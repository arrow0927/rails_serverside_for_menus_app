<h1>Listing temp_listings</h1>

<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>telephone</th>
    <th>fax</th>
    <th>suite</th>
    <th>address</th>
    <th>city</th>
    <th>province</th>
    <th>postal_code</th>
    <th>latitude</th>
    <th>longitude</th>
    <th>business Type</th>
    <th>Business Sub Type</th>
    <th>Owner</th>
    <th>Source</th>
    <th>neighbourhood</th>
  </tr>

<% @temp_listings.each do |temp_listing| %>
  <tr>
  	<th><%=temp_listing.id%></th>
    <th><%=temp_listing.name%></th>
    <th><%=temp_listing.telephone%></th>
    <th><%=temp_listing.fax%></th>
    <th><%=temp_listing.suite%></th>
    <th><%=temp_listing.address%></th>
    <th><%=temp_listing.city%></th>
    <th><%=temp_listing.province%></th>
    <th><%=temp_listing.postal_code%></th>
    <th><%=temp_listing.latitude%></th>
    <th><%=temp_listing.longitude%></th>
    <th><%=temp_listing.businessType%></th>
    <th><%=temp_listing.subType%></th>
    <th><%=temp_listing.owner%></th>
    <th><%=temp_listing.source%></th>
    <th><%=temp_listing.localArea%></th>
  	
  	
  	
  	
    <td><%= link_to 'Show', temp_listing %></td>
    <td><%= link_to 'Edit', edit_temp_listing_path(temp_listing) %></td>
    <td><%= link_to 'Destroy', temp_listing, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

