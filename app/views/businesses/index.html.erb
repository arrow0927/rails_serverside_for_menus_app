<h1>Listing businesses</h1>

<table>
  <tr>
    <th>name</th>
    <th>address</th>
    <th>city</th>
    <th>telephone1</th>
    <th>url</th>
    <th>fax</th>
    <th>telephone2</th>
    
  </tr>

<% @businesses.each do |business| %>
  <tr>
  	<td><%=business.name %> </td>
  	<td><%=business.address %> </td>
  	<td><%=business.city %> </td>
  	<td><%=business.telephone1 %> </td>
  	<td><%=business.url %> </td>
  	<td><%=business.fax %> </td>
  	<td><%=business.telephone2 %> </td>
  	
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Business', new_business_path %>
