<%= simple_form_for(@task) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :task_name %>
  </div>

    <div class="form-inputs">
      <%= f.input :description %>
    </div>


    <div class="form-inputs">
      <%= f.input :last_run_at_time %>
    </div>

    <div class="form-inputs">
      <%= f.input :last_run_by_user %>
    </div>

    <div class="form-inputs">
      <%= f.input :output_from_last_run %>
    </div>



  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
