<h1>Listing listings</h1>

<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>telephone</th>
    <th>fax</th>
    <th>suite</th>
    <th>address</th>
    <th>city</th>
    <th>province</th>
    <th>postal_code</th>
    <th>latitude</th>
    <th>longitude</th>
    <th>business Type</th>
    <th>Business Sub Type</th>
    <th>Owner</th>
    <th>Source</th>
    <th>neighbourhood</th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
  	
  	<th><%=listing.id%></th>
    <th><%=listing.name%></th>
    <th><%=listing.telephone%></th>
    <th><%=listing.fax%></th>
    <th><%=listing.suite%></th>
    <th><%=listing.address%></th>
    <th><%=listing.city%></th>
    <th><%=listing.province%></th>
    <th><%=listing.postal_code%></th>
    <th><%=listing.latitude%></th>
    <th><%=listing.longitude%></th>
    <th><%=listing.businessType%></th>
    <th><%=listing.subType%></th>
    <th><%=listing.owner%></th>
    <th><%=listing.source%></th>
    <th><%=listing.localArea%></th>
  	
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>   
    <td><%= link_to 'Destroy', listing, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
